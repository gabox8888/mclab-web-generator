{
	"outputType" : "ZIP",
	"command": "${config.MCVM_PATH} %mainFile @intCount @doubleNum*@test@test2 %secondaryFile %aspectFile",
	"name": "CompileMcMvm",
	"type": "FILE",
	"inFormat" : {
		"name": {
			"name" : "name",
			"position" : {
				"start" : {
					"line" : "startRow",
					"column" : "startColumn"
				},
				"end" : {
					"line" : "endRow",
					"column" : "endColumn"
				}
			}
		}
	},
	"outFormat" : {
		"name" : "name",
		"position" : {
			"startRow" : "startRow",
			"startColumn" : "startColumn",
			"endRow": "endRow",
			"endColumn" : "endColumn"
		}
	},
	"compileLanguage" : {
		"name" : "McMvm",
		"ext" : "generated.js"
	}
}
